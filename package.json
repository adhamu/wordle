    "predeploy": "yarn build",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "gh-pages": "^3.2.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@adhamu/zero": "^2.0.8",
    "@parcel/transformer-sass": "^2.0.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.0",
    "@types/jest": "^27.4.0",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "autoprefixer": "^10.4.2",
    "jest": "^27.0.5",
    "parcel": "^2.2.0",
    "postcss": "^8.0.9",
    "tailwindcss": "^3.0.15",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.4"
  }
}
